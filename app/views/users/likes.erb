<h1>お気に入り一覧</h1>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>id</th>
        <th>お気に入り</th>
      </tr>
    </thead>
    <tbody>
      <% @microposts.each do |micropost| %>
        <tr>
          <td><%= micropost.id %></td>
          <td><%= task.status %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <div>
    <%= paginate @micropost %>
  </div>
<% end %>